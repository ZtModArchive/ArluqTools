                                        <default>
                                            <BFFont align="center" />
                                        </default>
                                    </UIAspect>
                                    <text format="credits:xp2headingname" />
                                </UIText>
                                
                            </children>
                        </UIListBox>
                    </children>
                </UILayout>
            </children>
        </UILayout>
